{
  "name": "saransh-portfolio",
  "version": "1.0.0",
  "description": "Saransh Nimje's Full-Stack Portfolio - A modern web application showcasing projects, blog, and contact functionality",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "setup": "node setup.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "portfolio",
    "full-stack",
    "react",
    "nodejs",
    "express",
    "sql",
    "sequelize",
    "typescript",
    "tailwindcss",
    "framer-motion",
    "threejs",
    "blog",
    "contact",
    "projects"
  ],
  "author": "Saransh Nimje",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sharp": "^0.33.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saranshnimje/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/saranshnimje/portfolio/issues"
  },
  "homepage": "https://saranshnimje.com"
}
